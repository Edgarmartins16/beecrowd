#include <iostream>
#include <iomanip>

using namespace std;

int main() {
    double raio, area;
    const double pi = 3.14159;

    // Solicitar o raio como entrada
    cin >> raio;

    // Calcular a Ã¡rea
    area = pi * raio * raio;

    // Exibir o resultado com 4 casas decimais
    cout << "A=" << fixed << setprecision(4) << area << endl;

    return 0;
}
